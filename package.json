{
  "name": "next-typescript-starter",
  "private": true,
  "version": "0.1.0",
  "description": "Next + TypeScript + TailwindCSS starter.",
  "author": "Kyle Brumm <kjbrumm@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx}\"",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.1.0",
    "classnames": "^2.3.1",
    "next": "^12.1.0",
    "polished": "^4.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.41",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "husky": "^7.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  }
}
